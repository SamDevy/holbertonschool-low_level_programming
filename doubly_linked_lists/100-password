#include <stdio.h>
#include <string.h>

/**
 * main - checks if the password is correct
 * @argc: argument count
 * @argv: argument vector
 *
 * Return: 0 on success, 1 on failure
 */
int main(int argc, char *argv[])
{
    /* The program expects exactly one argument (the password) */
    if (argc != 2)
    {
        printf("Usage: %s <password>\n", argv[0]);
        return (1);
    }

    /*
     * The hint "do it with lists.h file" implies
     * the password is the literal string "lists.h"
     */
    if (strcmp(argv[1], "lists.h") == 0)
    {
        printf("OK\n");
    }
    else
    {
        printf("Wrong password\n");
    }

    return (0);
}
